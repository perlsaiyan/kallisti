#class {map} {kill}
#class {map} {open}

#alias mapRoom {
    #map vnum $MSDP_ROOM_VNUM;
    #map set roomname $MSDP_ROOM_NAME;
}

#action {~^\e\[1;35m%1%s\e} {
    event_raise mapEnterRoom {%1}.
}

#alias mapEnterRoom {
    #nop
}

event_register {mapEnterRoom} {mapEnterRoom} {mapEnterRoom %0}

#class {map} {close}

#nop -- vim: syntax=tt
