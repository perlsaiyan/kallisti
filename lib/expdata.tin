#tab levelreq
#alias {levelreq} {
	#unvar endlevel;
	#unvar startlevel;
	#var exp_tmp 0;
	#var gold_tmp 0;
	#switch {"%2"} {
		#case {""} {
			#switch {"%1"} {
				#case {""} {info data Usage: levelreq <'start level'> <'end level'>. You must enter at least one argument.};
				#default {diaLog info switch 1 default;
					#var endlevel %1;
					#var startlevel $MSDP_LEVEL
				}
			}
		};
		#default {diaLog info switch 2 default;
			#var endlevel %2;
			#var startlevel %1
		}
	};
	diaLog info checking start and end levels next;
	#if {&endlevel} {
		#if {$endlevel<=$startlevel} {
				info data End level must be greater than start level.
			};
		#elseif {$endlevel>$startlevel} {
			#math _istart $startlevel+1;
			#loop {$_istart} {$endlevel} {_i} {
				diaLog info Doing loop;
				#math exp_tmp {$exp_tmp+$explevel[$_i]};
				#math gold_tmp {$gold_tmp+$goldlevel[$_i]}
			};
			#var gold_tmp @hNum{$gold_tmp};
			info data From level '$startlevel' to '$endlevel' you need '@hNum{$exp_tmp}' exp, '$gold_tmp' gold and '$herolevel[$endlevel]' herps total.
		};
	}
}


